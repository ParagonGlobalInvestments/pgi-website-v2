{
  "version": 2,
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/:path*.html", "destination": "/", "permanent": false },
    { "source": "/:path*.htm", "destination": "/", "permanent": false }
  ]
}
